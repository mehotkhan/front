{
  "name": "mamoochi",
  "version": 1,
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.22.0",
  "scripts": {
    "build": "velite build & nuxt build",
    "build:local": "node --max-old-space-size=8192 node_modules/nuxt/bin/nuxt.mjs build",
    "analyze": "node --max-old-space-size=8192 node_modules/nuxt/bin/nuxt.mjs analyze",
    "clean": "rm -rf .nuxt dist .output dist_build .data .velite",
    "dev": "nuxt dev & velite dev",
    "generate": "nuxt generate",
    "i18:update": "node scripts/updateLocaleJS.js",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "check": "pnpm lint && pnpm type-check",
    "preview": "nuxt preview",
    "reset": "rm -rf .nuxt dist .output dist_build .wrangler .data .velite "
  },
  "dependencies": {
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^2.0.1",
    "@nuxt/image": "^2.0.0",
    "@nuxt/scripts": "^0.13.0",
    "@nuxt/ui": "4.2.0",
    "@nuxtjs/fontaine": "^0.5.0",
    "@nuxtjs/i18n": "10.2.1",
    "@nuxtjs/mdc": "^0.18.4",
    "@nuxtjs/robots": "^5.5.6",
    "@nuxtjs/seo": "3.2.2",
    "@nuxtjs/sitemap": "^7.4.7",
    "@nuxtjs/turnstile": "^1.1.1",
    "@vueuse/core": "^13.9.0",
    "better-sqlite3": "^12.4.1",
    "dexie": "^4.2.1",
    "drizzle-orm": "^0.44.7",
    "echarts": "^6.0.0",
    "echarts-liquidfill": "^3.1.0",
    "h3-zod": "^0.5.3",
    "luxon": "^3.7.2",
    "minisearch": "^7.2.0",
    "nuxt": "^4.2.1",
    "nuxt-auth-utils": "0.5.25",
    "nuxt-authorization": "0.3.5",
    "nuxt-echarts": "^1.0.1",
    "sass": "^1.94.1",
    "vue": "latest",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251118.0",
    "@iconify-json/lucide": "^1.2.73",
    "@nuxt/devtools": "^3.1.0",
    "@nuxt/eslint": "^1.10.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.1",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "nitro-cloudflare-dev": "^0.2.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "velite": "^0.3.0",
    "vue-router": "^4.6.3",
    "wrangler": "^4.49.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "unrs-resolver",
      "workerd"
    ],
    "supportedArchitectures": {
      "cpu": [
        "current",
        "x64"
      ],
      "os": [
        "current",
        "linux"
      ]
    },
    "overrides": {
      "sharp": "0.33.4"
    },
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "sharp",
      "vue-demi"
    ],
    "peerDependencyRules": {
      "allowedVersions": {
        "@nuxt/scripts": "0.13"
      }
    }
  }
}
